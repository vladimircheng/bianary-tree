#include <iostream>
#include <cstring>
#include <algorithm>
#include <queue>

using namespace std;
int cnt;
priority_queue<pair<int,string>,vector<pair<int,string> >,greater<pair<int,string> > > Q;

int main(){
    string a;
    while(cin>>a&&a!="\0"){
        //if(uwlist.is_uw(a))
        //continue;
        //遍历单词链表输出并入队位置信息
        //Q.push(make_pair(位置信息,a));
    }

    while(!Q.empty()){
        int i=Q.top().first;
        string j=Q.top().second;
        Q.pop();
        if((Q.top().first-i)==1)
        cout<<j<<' '<<Q.top().second<<' '<<"文档位置相邻"<<i<<' '<<Q.top().first<<endl;
    }
}
